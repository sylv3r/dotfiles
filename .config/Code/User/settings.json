{
  "workbench.iconTheme": "material-icon-theme",
  "vim.useCtrlKeys": false,
  "window.zoomLevel": 0,
  "terminal.integrated.fontSize": 11,
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 79,
  "editor.minimap.maxColumn": 79,
  "workbench.colorTheme": "Palenight Operator",
  "material-icon-theme.showUpdateMessage": false,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Kode Mono', 'Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
  "editor.cursorStyle": "block",
  "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/_build": true,
      "**/_deps": true,
      "**/.DS_Store": true
  },
  "editor.snippetSuggestions": "top",
  "editor.fontSize": 12,
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": true,
  "explorer.openEditors.visible": 0,
  "editor.tabCompletion": true,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "terminal.integrated.scrollback": 700,
  "window.title": "${dirty}${rootName}${separator}${activeEditorShort}${separator}",
  "explorer.confirmDragAndDrop": false,
  "editor.rulers": [
    79
  ],
  "latex-workshop.latex.toolchain": [
    {
      "command": "latexmk",
      "args": [
        "-pdf",
        "-pdflatex=xelatex",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-use-make",
        "%DOC%"
      ]
    }
  ],
  "terminal.integrated.shellArgs.linux": [
    "-i"
  ],
  "markdown.preview.fontSize": 12,
  "editor.minimap.enabled": false
}
